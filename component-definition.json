{
  "groups": [
    {
      "title": "Default Content",
      "id": "default",
      "components": [
        {
          "title": "Text",
          "id": "text",
          "plugins": {
            "da": {
              "name": "text",
              "type": "text"
            }
          }
        },
        {
          "title": "Image",
          "id": "image",
          "plugins": {
            "da": {
              "name": "image",
              "type": "image"
            }
          }
        }
      ]
    },
    {
      "title": "Sections",
      "id": "sections",
      "components": [
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<div></div>"
            }
          },
          "filter": "section",
          "model": "section"
        }
      ]
    },
    {
      "title": "Blocks",
      "id": "blocks",
      "components": [
        {
          "title": "Accordion",
          "id": "accordion",
          "plugins": {
            "da": {
              "name": "accordion",
              "rows": 1,
              "columns": 2
            }
          }
        },
        {
          "title": "Accordion Item",
          "id": "accordion-item",
          "plugins": {
            "da": {
              "name": "accordion-item",
              "rows": 2,
              "columns": 0
            }
          }
        },
        {
          "title": "Cards",
          "id": "cards",
          "plugins": {
            "da": {
              "rows": "variable",
              "columns": 2
            }
          }
        },
        {
          "title": "Carousel",
          "id": "carousel",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"carousel\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Carousel Slide",
          "id": "carousel-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture></div><div></div></div>"
            }
          }
        },
        {
          "title": "Columns",
          "id": "columns",
          "plugins": {
            "da": {
              "rows": 1,
              "columns": "variable"
            }
          }
        },
        {
          "title": "Fragment",
          "id": "fragment",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"fragment\"><div><div><p><a href=\"\">Fragment URL</a></p></div></div></div>"
            }
          }
        },
        {
          "title": "Hero",
          "id": "hero",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"hero\"><div><div><picture><source type=\"image/webp\" srcset=\"\"><source type=\"image/jpeg\" srcset=\"\"><img loading=\"lazy\"></picture><h1></h1></div></div></div>"
            }
          }
        },
        {
          "title": "Quote",
          "id": "quote",
          "plugins": {
            "da": {
              "name": "quote",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Search",
          "id": "search",
          "plugins": {
            "da": {
              "name": "search",
              "rows": 1,
              "columns": 1
            }
          }
        },
        {
          "title": "Section",
          "id": "section",
          "plugins": {
            "da": {
              "unsafeHTML": "<p>Section container for blocks. Use the + button to add blocks to this section.</p>"
            }
          }
        },
        {
          "title": "Tabs",
          "id": "tabs",
          "plugins": {
            "da": {
              "unsafeHTML": "<div class=\"tabs\"><div><div><p></p></div><div></div></div></div>"
            }
          }
        },
        {
          "title": "Tabs Item",
          "id": "tabs-item",
          "plugins": {
            "da": {
              "unsafeHTML": "<div><div><p></p></div><div></div></div>"
            }
          }
        },
        {
          "title": "Video",
          "id": "video",
          "plugins": {
            "da": {
              "name": "video",
              "rows": 1,
              "columns": 1
            }
          }
        }
      ]
    }
  ]
}